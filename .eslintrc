{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true,
    "es6": true,
    "jasmine": true
  },
  "globals": {
    "angular": false,
    "inject": false
  },
  "ignorePatterns": ["**/dist/*", "**/docs/*"],
  "rules": {
    "no-bitwise": 2,
    "curly": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "block-scoped-var": 0,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "new-cap": 0,
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "valid-typeof": 0,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "semi": 0,
    "no-cond-assign": 0,
    "no-debugger": 0,
    "no-eq-null": 2,
    "no-eval": 0,
    "no-unused-expressions": 0,
    "no-loop-func": 0,
    "no-proto": 0,
    "no-script-url": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-invalid-this": 0,
    "no-with": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-str": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "ignore",
        "named": "never"
      }
    ],
    "no-spaced-func": 2,
    "array-bracket-spacing": [
      2,
      "never",
      {}
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-trailing-spaces": 2,
    "comma-style": [
      2,
      "last"
    ],
    "eol-last": 2,
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "linebreak-style": [
      2,
      "unix"
    ]
  }
}
